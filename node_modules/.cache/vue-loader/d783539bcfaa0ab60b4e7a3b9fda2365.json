on>\n        <option value=\"GINE-HISTERIOS+CARTAS\">GINE-HISTERIOS+CARTAS</option>\n        <option value=\"DIGESTIVO (120)\">DIGESTIVO (120)</option>\n        <option value=\"DIGESTIVO (121)\">DIGESTIVO (121)</option>\n        <option value=\"3ª sala DIGESTIVO\">3ª sala DIGESTIVO</option>\n        <option value=\"HDQ\">HDQ</option>\n        <option value=\"HALL\">HALL</option>\n        <option value=\"UROLOGÍA\">UROLOGÍA</option>\n        <option value=\"NEUMOLOGÍA\">NEUMOLOGÍA</option>\n        <option value=\"OFTALMOLOGÍA\">OFTALMOLOGÍA</option>\n        <option value=\"REFUERZO\">REFUERZO</option>\n        <option value=\"EXTRACCIONES\">EXTRACCIONES</option>\n        <!-- Agregar todas las demás opciones -->\n      </select>\n      <input v-model=\"form.destino\" placeholder=\"Destino\" required />\n      <input v-model=\"form.condiciones\" placeholder=\"Condiciones\" required />\n      <select v-model=\"form.aislamiento\">\n        <option value=\"Sí\">Sí</option>\n        <option value=\"No\">No</option>\n      </select>\n      <input v-model=\"form.motivo\" placeholder=\"Motivo del traslado\" required />\n      <select v-model=\"form.prioridad\">\n        <option>Alta</option>\n        <option>Media</option>\n        <option>Baja</option>\n      </select>\n      <button type=\"submit\">Enviar</button>\n    </form>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      form: {\n        ubicacion: \"\",\n        destino: \"\",\n        condiciones: \"\",\n        aislamiento: \"No\",\n        motivo: \"\",\n        prioridad: \"Media\"\n      },\n      socket: null\n    };\n  },\n  mounted() {\n    this.socket = new WebSocket(\"ws://localhost:3000\");\n  },\n  methods: {\n    enviar() {\n      const mensaje = {\n        type: 'traslado',\n        ubicacion: this.form.ubicacion,\n        destino: this.form.destino,\n        condiciones: this.form.condiciones,\n        aislamiento: this.form.aislamiento,\n        motivo: this.form.motivo,\n        prioridad: this.form.prioridad\n      };\n      console.log(\"Enviando mensaje:\", mensaje);\n      this.socket.send(JSON.stringify(mensaje));\n      alert(\"Formulario enviado\");\n    },\n  }\n};\n</script>\n"]}]}