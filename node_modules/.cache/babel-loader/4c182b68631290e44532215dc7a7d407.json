        <button @click=\"iniciarTraslado(traslado.id)\">Iniciar Traslado</button>\r\n          </div>\r\n        </div>\r\n        <p v-else>No hay traslados pendientes.</p>\r\n      </section>\r\n  \r\n      <!-- ðŸŸ¢ SecciÃ³n: Traslados en Curso -->\r\n      <section>\r\n        <h3>Traslados en Curso</h3>\r\n        <div v-if=\"enCurso.length > 0\">\r\n          <div v-for=\"traslado in enCurso\" :key=\"traslado.id\" class=\"traslado-item\">\r\n            <p><strong>Origen:</strong> {{ traslado.ubicacion }}</p>\r\n            <p><strong>Destino:</strong> {{ traslado.destino }}</p>\r\n            <p><strong>Celador asignado:</strong> {{ traslado.celador_id || \"Sin asignar\" }}</p>\r\n            <button @click=\"finalizarTraslado(traslado.id)\">Finalizar Traslado</button>\r\n          </div>\r\n        </div>\r\n        <p v-else>No hay traslados en curso.</p>\r\n      </section>\r\n  \r\n      <!-- âœ… SecciÃ³n: Traslados Finalizados -->\r\n      <section>\r\n        <h3>Traslados Finalizados</h3>\r\n        <div v-if=\"finalizados.length > 0\">\r\n          <div v-for=\"traslado in finalizados\" :key=\"traslado.id\" class=\"traslado-item\">\r\n            <p><strong>Origen:</strong> {{ traslado.ubicacion }}</p>\r\n            <p><strong>Destino:</strong> {{ traslado.destino }}</p>\r\n            <p><strong>Celador asignado:</strong> {{ traslado.celador_id || \"Sin asignar\" }}</p>\r\n          </div>\r\n        </div>\r\n        <p v-else>No hay traslados finalizados.</p>\r\n      </section>\r\n  \r\n      <!-- ðŸ”Ž SecciÃ³n: Detalles de Todos los Traslados -->\r\n      <section>\r\n        <h3>Detalles de Traslados</h3>\r\n        <div v-if=\"detalles.length > 0\">\r\n          <div v-for=\"traslado in detalles\" :key=\"traslado.id\" class=\"traslado-item\">\r\n            <p><strong>Origen:</strong> {{ traslado.ubicacion }}</p>\r\n            <p><strong>Destino:</strong> {{ traslado.destino }}</p>\r\n            <p><strong>Celador asignado:</strong> {{ traslado.celador_id || \"Sin asignar\" }}</p>\r\n          </div>\r\n        </div>\r\n        <p v-else>No hay detalles disponibles.</p>\r\n      </section>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        pendientes: [],\r\n        enCurso: [],\r\n        finalizados: [],\r\n        detalles: [],\r\n      };\r\n    },\r\n    mounted() {\r\n      this.cargarTraslados();\r\n    },\r\n    methods: {\r\n      cargarTraslados() {\r\n        fetch(\"http://localhost:3000/traslados/pendientes\")\r\n          .then(res => res.json())\r\n          .then(data => this.pendientes = data);\r\n  \r\n        fetch(\"http://localhost:3000/traslados/enCurso\")\r\n          .then(res => res.json())\r\n          .then(data => this.enCurso = data);\r\n  \r\n        fetch(\"http://localhost:3000/traslados/finalizados\")\r\n          .then(res => res.json())\r\n          .then(data => this.finalizados = data);\r\n  \r\n        fetch(\"http://localhost:3000/traslados/detalles\")\r\n          .then(res => res.json())\r\n          .then(data => this.detalles = data);\r\n      },\r\n      iniciarTraslado(trasladoId) {\r\n        // Encontrar el traslado pendiente en base al ID\r\n        const trasladoIndex = this.pendientes.findIndex(traslado => traslado.id === trasladoId);\r\n        \r\n        if (trasladoIndex !== -1) {\r\n          // Moverlo a los enCurso\r\n          const traslado = this.pendientes.splice(trasladoIndex, 1)[0];\r\n          this.enCurso.push(traslado);\r\n  \r\n          // Realizar el cambio en el backend\r\n          fetch(`http://localhost:3000/traslados/iniciar/${trasladoId}`, {\r\n            method: 'POST',\r\n            headers: {\r\n              'Content-Type': 'application/json',\r\n            },\r\n            body: JSON.stringify(traslado),\r\n          })\r\n          .then(res => res.json())\r\n          .then(data => {\r\n            console.log(\"Traslado en curso actualizado en el backend:\", data);\r\n          })\r\n          .catch(error => {\r\n            console.error(\"Error al iniciar traslado:\", error);\r\n          });\r\n        }\r\n      },\r\n      finalizarTraslado(trasladoId) {\r\n        // Encontrar el traslado en curso en base al ID\r\n        const trasladoIndex = this.enCurso.findIndex(traslado => traslado.id === trasladoId);\r\n  \r\n        if (trasladoIndex !== -1) {\r\n          // Moverlo a los finalizados\r\n          const traslado = this.enCurso.splice(trasladoIndex, 1)[0];\r\n          this.finalizados.push(traslado);\r\n  \r\n          // Actualizar el estado en el backend\r\n          fetch(`http://localhost:3000/traslados/finalizar/${trasladoId}`, {\r\n            method: 'POST',\r\n            headers: {\r\n              'Content-Type': 'application/json',\r\n            },\r\n            body: JSON.stringify(traslado),\r\n          })\r\n          .then(res => res.json())\r\n          .then(data => {\r\n            console.log(\"Traslado finalizado en el backend:\", data);\r\n          })\r\n          .catch(error => {\r\n            console.error(\"Error al finalizar traslado:\", error);\r\n          });\r\n        }\r\n      },\r\n    }\r\n  };\r\n  </script>\r\n  "],"mappings":"AA6DA;EACAA,KAAA;IACA;MACAC,UAAA;MACAC,OAAA;MACAC,WAAA;MACAC,QAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,eAAA;EACA;EACAC,OAAA;IACAD,gBAAA;MACAE,KAAA,+CACAC,IAAA,CAAAC,GAAA,IAAAA,GAAA,CAAAC,IAAA,IACAF,IAAA,CAAAT,IAAA,SAAAC,UAAA,GAAAD,IAAA;MAEAQ,KAAA,4CACAC,IAAA,CAAAC,GAAA,IAAAA,GAAA,CAAAC,IAAA,IACAF,IAAA,CAAAT,IAAA,SAAAE,OAAA,GAAAF,IAAA;MAEAQ,KAAA,gDACAC,IAAA,CAAAC,GAAA,IAAAA,GAAA,CAAAC,IAAA,IACAF,IAAA,CAAAT,IAAA,SAAAG,WAAA,GAAAH,IAAA;MAEAQ,KAAA,6CACAC,IAAA,CAAAC,GAAA,IAAAA,GAAA,CAAAC,IAAA,IACAF,IAAA,CAAAT,IAAA,SAAAI,QAAA,GAAAJ,IAAA;IACA;IACAY,gBAAAC,UAAA;MACA;MACA,MAAAC,aAAA,QAAAb,UAAA,CAAAc,SAAA,CAAAC,QAAA,IAAAA,QAAA,CAAAC,EAAA,KAAAJ,UAAA;MAEA,IAAAC,aAAA;QACA;QACA,MAAAE,QAAA,QAAAf,UAAA,CAAAiB,MAAA,CAAAJ,aAAA;QACA,KAAAZ,OAAA,CAAAiB,IAAA,CA